<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/color_back_navi">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/img"
            android:src="@drawable/ic_navigationicon" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_baseline_home_24"
            android:layout_alignParentRight="true"
            android:layout_marginRight="20dp"
            android:scaleType="fitXY"
            android:layout_centerVertical="true"/>

    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_margin="20dp">

        <RelativeLayout
            android:layout_width="250dp"
            android:layout_height="40dp"
            android:id="@+id/home"
            android:background="@drawable/back_rectangle_gray">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="Home"
                android:textSize="17sp"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:drawableLeft="@drawable/ic_baseline_home_24"
                android:layout_marginLeft="20dp"
                android:drawablePadding="7dp"/>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="250dp"
            android:layout_height="40dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/back_rectangle_gray">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="All Survey"
                android:textSize="17sp"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:drawableLeft="@drawable/ic_baseline_home_24"
                android:layout_marginLeft="20dp"
                android:drawablePadding="7dp"/>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="250dp"
            android:layout_height="40dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/back_rectangle_gray">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="Reports"
                android:textSize="17sp"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:drawableLeft="@drawable/ic_baseline_home_24"
                android:layout_marginLeft="20dp"
                android:drawablePadding="7dp"/>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="250dp"
            android:layout_height="40dp"
            android:layout_marginTop="20dp"
            android:id="@+id/noti"
            android:background="@drawable/back_rectangle_gray">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:drawableLeft="@drawable/ic_baseline_home_24"
                android:drawablePadding="7dp"
                android:text="Notification"
                android:textColor="@color/white"
                android:textSize="17sp"
                android:textStyle="bold" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="250dp"
            android:layout_height="40dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/back_rectangle_gray">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="Share"
                android:textSize="17sp"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:drawableLeft="@drawable/ic_baseline_home_24"
                android:layout_marginLeft="20dp"
                android:drawablePadding="7dp"/>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="250dp"
            android:layout_height="40dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/back_rectangle_gray">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="Term of Use"
                android:textSize="17sp"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:drawableLeft="@drawable/ic_baseline_home_24"
                android:layout_marginLeft="20dp"
                android:drawablePadding="7dp"/>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="250dp"
            android:layout_height="40dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/back_rectangle_gray">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="Privacy Policy"
                android:textSize="17sp"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:drawableLeft="@drawable/ic_baseline_home_24"
                android:layout_marginLeft="20dp"
                android:drawablePadding="7dp"/>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="250dp"
            android:layout_height="40dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/back_rectangle_gray">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="Log-out"
                android:textSize="17sp"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:drawableLeft="@drawable/ic_baseline_home_24"
                android:layout_marginLeft="20dp"
                android:drawablePadding="7dp"/>

        </RelativeLayout>

    </LinearLayout>

</LinearLayout>